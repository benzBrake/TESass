{
  "name": "tesass",
  "version": "1.0.0",
  "description": "Use Sass with Typecho, Node Script in SCSS flavour.",
  "scripts": {
    "css": "node-sass --output-style compressed --include-path scss assets/css/source.scss style.css",
    "watch": "nodemon -e scss -x \"npm run css\""
